{
  "expo": {
    "name": "eos-report-integrated-pomodoro-app-client",
    "slug": "eos-report-integrated-pomodoro-app-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.kebluis.eosreportintegratedpomodoroappclient",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "yellow",
          "sounds": [
            "./assets/alarm.wav"
          ]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kebluis.eosreportintegratedpomodoroappclient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "package": "com.kebluis.eosreportintegratedpomodoroappclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ccd5ac9c-6e6a-45ea-807d-baff28f1b327"
      }
    }
  }
}
